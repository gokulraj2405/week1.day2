package week1.day2;

public class PositiveNumber {

	public static void main(String[] args) {
		int num=0;
		if (num>0) {
			System.out.println("positive number");
			
		}
		else if (num==0) {
			System.out.println("The number is neither positive nor negative");
			
		}
		
		else {
			System.out.println("not a positive number");

		}

		
	
}

	

}
